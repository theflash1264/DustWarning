@model DustWarning.ViewModel._Business.Business_OutboundSettingVMs.Business_OutboundSettingVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
	<wt:quote>@Localizer["Sys.DeleteConfirm"]</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.IsCall" />
<wt:display field="Entity.EcName" />
<wt:display field="Entity.ApId" />
<wt:display field="Entity.SecretKey" />
<wt:display field="Entity.Sign" />
<wt:display field="Entity.Username" />
<wt:display field="Entity.ApiKey" />
<wt:display field="Entity.EntId" />
<wt:display field="Entity.RobotNum" />
<wt:display field="Entity.OutCallers" />
<wt:display field="Entity.CreateBy" />
<wt:display field="Entity.CreateUser" />
<wt:display field="Entity.SurplusAmount" />
<wt:display field="Entity.UnitPrice" />
<wt:display field="Entity.CallInterval" />
<wt:display field="Entity.TenantCode" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="@Localizer["Sys.Delete"]"/>
        <wt:closebutton />
    </wt:row>
</wt:form>
